{
  "name": "sakenowa-openapi",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "pnpm /^watch:.*/",
    "watch:tsp": "tsp compile ./ --watch",
    "watch:redocly": "redocly preview-docs",
    "build": "pnpm build:tsp && pnpm build:redocly",
    "build:tsp": "tsp compile ./",
    "build:redocly": "redocly build-docs -o './dist/docs/index.html'",
    "prebuild": "pnpx rimraf ./dist",
    "check": "pnpm /^check:.*/",
    "check:format": "tsp format -c ./",
    "check:lint": "redocly lint",
    "fix": "pnpm /^fix:.*/",
    "fix:format": "tsp format ./"
  },
  "devDependencies": {
    "@redocly/cli": "^1.34.3",
    "@typespec/compiler": "^1.0.0",
    "@typespec/http": "^1.0.1",
    "@typespec/openapi3": "^1.0.0"
  },
  "private": true
}
